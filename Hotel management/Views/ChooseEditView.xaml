<Window x:Class="Hotel_management.Views.ChooseEditView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hotel_management.Views"
        mc:Ignorable="d"
        Title="ChooseEditView" Height="200" Width="600">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <Button Content="User" Width="100" Height="50" Margin="50,0,0,0" Click="Button_Click"/>
            <Button Content="Rooms" Width="100" Height="50" Margin="50,0,0,0" Click="Button_Click_1"/>
        </StackPanel>
    </Grid>
</Window>
